{
    "manifest_version": 3,
    "name": "Acube.SwitchTab",
    "version": "1.0.0",
    "description": "Switch tab like Windows.",
    "background": {
        "service_worker": "background.js"
    },
    "permissions": [
        "tabs",
        "commands"
    ],
    "commands": {
        "switch_tab": {
            "suggested_key": {
                "default": "Alt+W"
            },
        "description": "Switch to the most recently used tab."
        }
    }
}